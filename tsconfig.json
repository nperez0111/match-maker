{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "types": ["@types/bun"],
    "paths": {
      "match-maker/encryption-key": ["./src/encryption-key/index.ts"],
      "match-maker/storage": ["./src/storage/index.ts"],
      "match-maker/transports": ["./src/transports/index.ts"],
      "match-maker/server": ["./src/server/index.ts"],
      "match-maker/protocol": ["./src/protocol/index.ts"],
      "match-maker/providers": ["./src/providers/index.ts"],
      "match-maker": ["./src/lib/index.ts"],
    },
    "jsx": "react-jsx"
  },
  "include": ["src", "playground"]
}
